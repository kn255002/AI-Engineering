    "import numpy as np\n",
    "\n",
    "    \n",
    "# replace numbers with \"YES\" if it divided by 3 and 5\n",
    "    # otherwise it will be replaced with \"NO\"\n",
    "    # Hint: np.where\n",
    "arr = np.arange(1,10*10+1).reshape((10,10))\n",
    "print(arr)\n",
    "a=np.where((arr % 3==0) | (arr % 5==0), 'YES','NO')\n",
    "a"
   ]
